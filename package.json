{
  "name": "gatsby-plugin-typesense",
  "version": "0.0.1",
  "description": "A gatsby plugin to index site content to Typesense",
  "main": "index.js",
  "scripts": {
    "testground:build": "npm link && cd test/support/testground && npm install && npm link gatsby-plugin-typesense && gatsby build",
    "typesenseServer": "docker run -i -p 8108:8108 -v/tmp/typesense-server-data/:/data typesense/typesense:0.15.0 --data-dir /data --api-key=xyz --listen-port 8108 --enable-cors",
    "test": "npm run testground:build && jest"
  },
  "keywords": [
    "gatsby",
    "gatsby-plugin",
    "typesense",
    "search"
  ],
  "author": "Jason Bosco <jason@typesense.org>",
  "license": "Apache-2.0",
  "devDependencies": {
    "@babel/runtime": "^7.10.2",
    "axios-mock-adapter": "^1.18.2",
    "jest": "^26.5.3",
    "rewire": "^5.0.0"
  },
  "dependencies": {
    "cheerio": "^1.0.0-rc.3",
    "prettier": "^2.1.2",
    "typesense": "^0.7.1"
  },
  "peerDependencies": {
    "@babel/runtime": "^7.10.2"
  }
}
